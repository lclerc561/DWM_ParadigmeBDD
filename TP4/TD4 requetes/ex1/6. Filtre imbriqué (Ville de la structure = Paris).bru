meta {
  name: 6. Filtre imbriqu√© (Ville de la structure = Paris)
  type: graphql
  seq: 6
}

get {
  url: http://localhost:8055/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  query {
    Praticien(filter: { structure: { ville: { _eq: "Paris" } } }) {
      nom
      structure {
        ville
      }
    }
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
