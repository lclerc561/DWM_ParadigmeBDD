meta {
  name: 3. Filtre sur la ville (Paris)
  type: graphql
  seq: 3
}

get {
  url: http://localhost:8055/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  query {
    Praticien(filter: { ville: { _eq: "Paris" } }) {
      nom
      prenom
      ville
    }
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
